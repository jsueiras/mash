                          rdfs:domain         :GuardianPerson ;
                          rdfs:label          "guardianInHousehold" ;
                          rdfs:subPropertyOf  mash:inHousehold ;
                          rdfs:comment        "this is a hack to make test data load easier".

:guardianParent           a                   owl:ObjectProperty ;
                          rdfs:domain         :GuardianPerson ;
                          rdfs:label					"parent";
