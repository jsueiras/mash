<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.mash.data.com/mashModel" 
xmlns:tns="http://www.mash.data.com/mashModel"
 xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  jaxb:version="2.0"
  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
  jaxb:extensionBindingPrefixes="xjc" elementFormDefault="qualified">

  <annotation>
        <appinfo>
            <jaxb:globalBindings>
                <xjc:simple />
            </jaxb:globalBindings>
        </appinfo>
    </annotation>
   <complexType name="Root" abstract="true">
       <sequence>
          <element name="id" type="string" minOccurs="0"></element>
       </sequence>
   </complexType>
   <complexType name="Act" abstract="true">
     <complexContent>
   		<extension base="tns:Root">
        <sequence>
   		    <element name="ocurrenceDate" type="date" minOccurs="0"></element>
   	        <element name="participants" type="tns:Participations" minOccurs="0"></element>
   	   </sequence>
   	   </extension>
   	   </complexContent>
   </complexType>
   <complexType name="Entity" abstract="true">
      <complexContent>
   		<extension base="tns:Root">
      </extension>
      </complexContent>		
   </complexType>
     <complexType name="EntitySummary" abstract="true">
      <complexContent>
   		<extension base="tns:Entity">
    		<sequence>
   			    <element name="type" type="string" minOccurs="1"/>
   				 <element name="attributes" type="tns:Entry" minOccurs="0" maxOccurs="unbounded"/>
   			</sequence>
       </extension>
      </complexContent>		
   </complexType>
   <complexType name="Role" abstract="true">
       <sequence>
           <element name="from" type="date" minOccurs="0"></element>
   		   <element name="to" type="date" minOccurs="0"></element>
       </sequence>
   </complexType>
     
     <complexType name="Relation" abstract="true">
      <complexContent>
   		<extension base="tns:Role">
   		   <sequence>
   		      <element ref="tns:person"/>
   		   </sequence>
      </extension>
      </complexContent>		
   </complexType>
   
   
     <complexType name="Parent">
      <complexContent>
   		<extension base="tns:Relation">
   		   
      </extension>
      </complexContent>		
   </complexType>
    <complexType name="Child">
      <complexContent>
   		<extension base="tns:Relation">
   		   
      </extension>
      </complexContent>		
   </complexType>
    <complexType name="OtherRelation">
    	<complexContent>
    		<extension base="tns:Relation">
                <sequence>
                  <element name="type" type="string" minOccurs="1"/>
                </sequence>
    		</extension>
    	</complexContent>
    </complexType>

    <complexType name="Sibiling">
      <complexContent>
   		<extension base="tns:Relation">
   		   
      </extension>
      </complexContent>		
   </complexType>
   
    <complexType name="Participation" abstract="true">
       
   </complexType>
   
    <complexType name="GenericParticipant" >
      <complexContent>
   		<extension base="tns:Participation">
   		    <choice>
   		      <element ref="tns:act"/>
   		      <element ref="tns:entity"/>
   		   </choice>
      </extension>
      </complexContent>	
   </complexType>
   <complexType name="Victim">
      <complexContent>
   		<extension base="tns:Participation">
   		   <choice>
   		      <element ref="tns:crime"/>
   		      <element ref="tns:person"/>
   		   </choice>
      </extension>
      </complexContent>		
   </complexType>
   
    <complexType name="SubjectOf">
      <complexContent>
   		<extension base="tns:Participation">
   		   <choice>
   		      <element ref="tns:referral"/>
   		      <element ref="tns:person"/>
   		   </choice>
      </extension>
      </complexContent>		
   </complexType>

  <complexType name="Crime">
   	<complexContent>
   		<extension base="tns:Act">
   			
   		</extension>
   	</complexContent>
   </complexType>
   
    <complexType name="ActSummary">
   	<complexContent>
   		<extension base="tns:Act">
   			<sequence>
   			    <element name="type" type="string" minOccurs="1"/>
   				 <element name="attributes" type="tns:Entry" minOccurs="0" maxOccurs="unbounded"/>
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>
   <complexType name="Referral">
   	<complexContent>
   		<extension base="tns:Act">
   			<sequence>
   			    <element name="type" type="string" minOccurs="0"/>
   				<element name="comments" type="string" minOccurs="0" maxOccurs="unbounded" />
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>
   
  
   
    <complexType name="Person">
   	<complexContent>
   		<extension base="tns:Entity">
   			<sequence>
   				<element name="firstName" type="string" minOccurs="0"></element>
   				<element name="lastName" type="string" minOccurs="0"></element>
   				<element name="dateOfBirth" type="date" minOccurs="0"></element>
   				<element name="gender" type="string" minOccurs="0"></element>
   				<element name="homeAddress" type="tns:Address" minOccurs="0"></element>
   				<element name="participations" type="tns:Participations" minOccurs="0"></element>
   				<element name="household" type="tns:Household" minOccurs="0"></element>
   				<element name="otherAddresses" type="tns:Address" minOccurs="0" maxOccurs="unbounded"></element>
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>
   
    <complexType name="Address">
   	<complexContent>
   		<extension base="tns:Role">
   			<sequence>
   				<element name="location" type="tns:Location" minOccurs="1"></element>
   				<element name="type" type="string" minOccurs="0"></element>
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>
   
    <complexType name="Occupant">
   	<complexContent>
   		<extension base="tns:Role">
   			<sequence>
   				<element name="person" type="tns:Person" minOccurs="1"></element>
   				<element name="type" type="string" minOccurs="0"></element>
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>
   
   <complexType name="Location">
   	<complexContent>
   		<extension base="tns:Entity">
   			<sequence>
   				<element name="uprn" type="string" minOccurs="0"></element>
   				<element name="numberOrName" type="string"  minOccurs="0"></element>
   				<element name="street" type="string" minOccurs="0"></element>
   				<element name="city" type="string"  minOccurs="0"></element>
   				<element name="county" type="string"  minOccurs="0"></element>
   				<element name="postcode" type="string"  minOccurs="0"></element>
   				<element name="occupants" type="tns:Occupant" minOccurs="0" maxOccurs="unbounded"></element>
   			</sequence>
   		</extension>
   	</complexContent>
   </complexType>


  
 
     <element name="locations" type="tns:Locations"/>
    
   <element name="persons" type="tns:Persons"/>
  
   <element name="entities" type="tns:Entities"/>
  
   
     
 
   
  <complexType name="Participations">
     <sequence>
        <element ref="tns:participation"  minOccurs="0" maxOccurs="unbounded"></element>
     </sequence>
  </complexType>
  
    
  <complexType name="Persons">
     <sequence>
        <element ref="tns:person"  minOccurs="0" maxOccurs="unbounded"></element>
     </sequence>
  </complexType>
  
  <complexType name="Locations">
     <sequence>
        <element ref="tns:location"  minOccurs="0" maxOccurs="unbounded"></element>
     </sequence>
  </complexType>
  
   <complexType name="Entities">
     <sequence>
        <element ref="tns:entity"  minOccurs="0" maxOccurs="unbounded"></element>
     </sequence>
  </complexType>
  
   <complexType name="Household">
     <sequence>
        <element ref="tns:relation"  minOccurs="0" maxOccurs="unbounded"></element>
     </sequence>
  </complexType>
  
   <element name="act" abstract="true" type="tns:Act"/>
   <element name="crime" type="tns:Crime" substitutionGroup="tns:act"/>
   <element name="referral" type="tns:Referral" substitutionGroup="tns:act"/>
    <element name="actSummary" type="tns:ActSummary" substitutionGroup="tns:act"/>
   
   <element name="entity" abstract="true" type="tns:Entity"/>
   <element name="person" type="tns:Person" substitutionGroup="tns:entity"/>
   <element name="location" type="tns:Location" substitutionGroup="tns:entity"/>
   <element name="entitySummary" type="tns:EntitySummary" substitutionGroup="tns:entity"/>
    
  
    
    
    <element name="relation" abstract="true" type="tns:Relation"/>
   
    
    <element name="parent" type="tns:Parent" substitutionGroup="tns:relation"/>
    <element name="child" type="tns:Child" substitutionGroup="tns:relation"/>
   
    <element name="sibiling" type="tns:Sibiling" substitutionGroup="tns:relation"/>
    <element name="otherRelation" type="tns:OtherRelation" substitutionGroup="tns:relation"/>
 
   <element name="participation" abstract="true" 
             type="tns:Participation"/>
   
   <element name="victim" substitutionGroup="tns:participation" 
             type="tns:Victim"/>
   <element name="subjectOf" substitutionGroup="tns:participation" 
             type="tns:SubjectOf"/>
    <element name="generic" substitutionGroup="tns:participation" 
             type="tns:GenericParticipant"/>          
             
 
  
     <complexType name="Entry">
           <sequence>
                <element name="key" minOccurs="1" type="string"/>
                 <element name="value" minOccurs="0" type="string"/>
           </sequence>
     </complexType>
</schema>